<!DOCTYPE html>
<html>
<head>
  <title>MentorMatch</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%=link_to image_tag("MentorMatch_Logo.png", alt: "MentorMatch", class: "logo clearfix" ), root_path%>

	<% if current_user %>

    <nav class="logged-in ">

      <ul>
        <li>
          <%= link_to "Profile", user_url(current_user) %>
        </li>
        <li>
           <%= link_to "mentee", "/users/mentees" %>
        </li>
         <li>
            <%= link_to "mentors", "/users/mentors" %>
        </li>
        <li>
          <%= link_to "Filter Search",  '/users/:user_id/filters/new' %>
        </li>
         <li>
          <%= button_to "Sign Out", session_url, method: "delete" %>
        </li>
      </ul>
    </nav>

  <% end %>
  

 <%= yield %>

<!-- <pre> session.to_hash.to_yaml </pre> -->
 
</body>
</html>
